{
  "name": "ats",
  "version": "1.0.0",
  "description": "AST Parser CLI - Index TypeScript/JavaScript files into an SQLite database",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "ats": "./bin/run.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepack": "npm run build",
    "start": "node ./bin/run.js",
    "dev": "node ./bin/dev.js",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "oclif": {
    "bin": "ats",
    "dirname": "ats",
    "commands": {
      "strategy": "pattern",
      "target": "./dist/commands"
    },
    "default": "parse",
    "topicSeparator": " "
  },
  "files": [
    "/bin",
    "/dist"
  ],
  "dependencies": {
    "@oclif/core": "^4.0.0",
    "better-sqlite3": "^12.6.2",
    "chalk": "^5.3.0",
    "glob": "^11.0.0",
    "tree-sitter": "^0.21.1",
    "tree-sitter-javascript": "^0.23.0",
    "tree-sitter-typescript": "^0.23.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.0.0",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "tree-sitter",
      "tree-sitter-javascript",
      "tree-sitter-typescript"
    ]
  },
  "license": "MIT"
}
